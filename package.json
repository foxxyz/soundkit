  },
  "peerDependencies": {
    "vue": ">2"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "dependencies": {
    "events": "^3.3.0",
    "nanoid": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/foxxyz/soundkit"
  },
  "devDependencies": {
    "@appliedminds/eslint-config": "^1.3.0",
    "eslint": "^8.30.0",
    "eslint-plugin-jest": "^27.1.7",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1"
  },
  "author": "Ivo Janssen <hello@ivo.la>",
  "contributors": [
    "Andrew Burke <andrewsburke@gmail.com>"
  ],
  "license": "MIT",
  "type": "module",
  "eslintConfig": {
    "env": {
      "browser": true,
      "jest/globals": true
    },
    "extends": [
      "@appliedminds"
    ],
    "plugins": [
      "jest"
    ]
  }
}
