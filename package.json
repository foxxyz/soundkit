{
  "name": "soundkit",
  "version": "3.0.0",
  "description": "Easy Sound Management & Playback",
  "main": "index.js",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "types/*",
    "index.js"
  ],
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint ."
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text"
    ],
    "setupFiles": [
      "./test/mocks.js"
    ],
    "testEnvironment": "jsdom",
    "transform": {},
